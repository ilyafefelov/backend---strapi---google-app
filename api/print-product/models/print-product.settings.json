{
  "kind": "collectionType",
  "collectionName": "PrintProduct",
  "info": {
    "name": "PrintProduct"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "material": {
      "type": "enumeration",
      "enum": [
        "textile",
        "paper",
        "plastic"
      ],
      "default": "textile",
      "required": true
    },
    "inventory": {
      "type": "biginteger",
      "required": true,
      "default": "1"
    },
    "pictures": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    },
    "Designer": {
      "type": "string"
    }
  }
}
